<%= form_for :task do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <%= hidden_field "task", "completed" %>
  <%= hidden_field "task_id", "183812984" %>
  <p>
    <%= f.submit %>
  </p>
<% end %>
